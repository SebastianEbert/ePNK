            </separator>
         </menu>
         <action
               class="org.pnml.tools.epnk.gmf.integration.actions.popup.RefreshImages"
               enablesFor="1"
               id="org.pnml.tools.epnk.gmf.integration.actions.refreshImageChache"
               label="Clear Image Cache"
